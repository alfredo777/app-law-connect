<?xml version='1.0' encoding='utf-8'?>
<widget id="com.laconnect.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>LEGAL CONNECT</name>
    <description>
        Encuentra asesor√≠a en derecho fiscal y laboral de acuerdo a tus necesides.
    </description>
    <author email="lawconnectapp@gmail.com" href="lawconnect.mx">
        LEGAL CONNECT
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
     <preference name="android-minSdkVersion"  value="16" />
     <preference name="android-targetSdkVersion"  value="26" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <config-file parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
            <string>LEGAL CONNECT  would like to store a photo for your AVATAR.</string>
        </config-file>
        <config-file parent="NSPhotoLibraryUsageDescription" platform="ios" target="*-Info.plist">
            <string>LEGAL CONNECT would like to use a photo for your AVATAR.</string>
        </config-file>
    </platform>
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="deployment-target" value="7.0" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />

    <platform name="android">
      <!--   <icon qualifier="ldpi" src="www/res/icons/android/drawable-ldpi-icon.png" /> -->
